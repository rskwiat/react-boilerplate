{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Personal React Boilerplate",
  "main": "webpack.config.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "devDependencies": {
    "jasmine-react-helpers": "^0.2.2",
    "react-addons-test-utils": "^15.3.1",
    "webpack-dashboard": "^0.1.8"
  },
  "scripts": {
    "build:js": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --devtool inline-source-map --history-api-fallback",
    "production:js": "node ./node_modules/webpack/bin/webpack.js --config webpack.production.js -p",
    "test": "NODE_ENV=test ./node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rskwiat/react-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rskwiat/react-boilerplate/issues"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "homepage": "https://github.com/rskwiat/react-boilerplate#readme"
}
